        bool "Support 1GB LPDDR4"
        help
         Enable support for IWG34M 1GB LPDDR4 platform

config SDRAM_SIZE_2GB
        bool "Support 2GB LPDDR4"
        help
         Enable support for IWG34M 2GB LPDDR4 platform

config SDRAM_SIZE_4GB
        bool "Support 4GB LPDDR4"
        help
         Enable support for IWG34M 4GB LPDDR4 platform

config TARGET_IMX8MM_IWG34M_UUU
        bool "imx8mm LPDDR4 IWG34M board UUU"
        help
         Enable UUU tool Support for IWG34M platform

config IMX_CONFIG
	default "board/freescale/imx8mm_iwg34m/imximage-8mm.cfg"

source "board/freescale/common/Kconfig"

